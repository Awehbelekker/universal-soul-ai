{
  "multimodal_ai": {
    "enabled": true,
    "primary_provider": "gemini_pro",
    "fallback_providers": [
      "local_vision"
    ],
    "gemini_settings": {
      "model": "gemini-1.5-flash",
      "temperature": 0.1,
      "max_tokens": 1000,
      "safety_settings": "default"
    }
  },
  "automation": {
    "confidence_threshold": 0.8,
    "max_retries": 3,
    "fallback_to_traditional_cv": true
  },
  "performance": {
    "cache_enabled": true,
    "batch_processing": false,
    "low_latency_mode": true
  }
}